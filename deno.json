{
  "name": "@tettuan/breakdownprompt",
  "version": "0.1.5",
  "exports": "./src/mod.ts",
  "tasks": {
    "test": "LOG_LEVEL=debug deno test --allow-read --allow-write --allow-env",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "publish": "deno publish"
  },
  "imports": {
    "@std/": "jsr:@std/",
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/log": "jsr:@std/log@^0.224.14",
    "@std/testing": "jsr:@std/testing@^1.0.9",
    "std/": "jsr:@std/",
    "testing/": "jsr:@std/testing/"
  },
  "compilerOptions": {
    "lib": [
      "deno.ns",
      "dom",
      "esnext"
    ],
    "strict": true,
    "types": [
      "deno-types"
    ]
  },
  "fmt": {
    "files": {
      "include": [
        "src/",
        "tests/"
      ]
    },
    "options": {
      "lineWidth": 100,
      "indentWidth": 2
    }
  },
  "publish": {
    "include": [
      "src/",
      "utils/",
      "README.md",
      "LICENSE"
    ],
    "exclude": [
      "tests/",
      "docs/",
      ".git/",
      ".cursor/"
    ]
  },
  "test": {
    "files": {
      "include": [
        "tests/"
      ]
    }
  }
}
